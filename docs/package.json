{
  "name": "sancho-docs",
  "description": "The sancho docs built with gatsby",
  "version": "0.1.0",
  "author": "Ben McMahen <ben.mcmahen@gmail.com>",
  "dependencies": {
    "@babel/standalone": "^7.3.4",
    "@emotion/styled": "^10.0.7",
    "@mdx-js/mdx": "^0.18.0",
    "@mdx-js/tag": "^0.18.0",
    "@types/react-helmet": "^5.0.8",
    "babel-plugin-emotion": "^10.0.7",
    "gatsby": "^2.1.18",
    "gatsby-image": "^2.0.30",
    "gatsby-mdx": "^0.4.0",
    "gatsby-plugin-emotion": "^4.0.4",
    "gatsby-plugin-layout": "^1.0.12",
    "gatsby-plugin-manifest": "^2.0.19",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-plugin-sharp": "^2.0.22",
    "gatsby-plugin-typescript": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-react-docgen": "^3.0.5",
    "gatsby-transformer-sharp": "^2.1.14",
    "prism-react-renderer": "^0.1.5",
    "prop-types": "^15.7.2",
    "react-component-component": "^1.2.1",
    "react-helmet": "^5.2.0",
    "react-live": "^1.12.0",
    "react-remove-scroll": "^1.0.4"
  },
  "devDependencies": {
    "babel-plugin-typescript-to-proptypes": "^0.17.1",
    "babel-preset-gatsby": "^0.1.8",
    "prettier": "^1.16.4",
    "react-docgen": "4.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build-for-docgen": "cd ../ && yarn run build-for-docgen",
    "generate-props": "yarn run build-for-docgen && react-docgen ../build -e /stories/ --out ./src/pages/components/props.json --resolver=findAllComponentDefinitions --cwd=../",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop-phone": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
